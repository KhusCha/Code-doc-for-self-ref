package uni.codingNinja;

import java.util.Scanner;

public class CodingNinja {
	
	public static void main(String[] args) {
		
        
        Scanner s = new Scanner(System.in);
		
		int x =1,sum,i=1;
		
		int n=s.nextInt();
		while(x<=n && n<=1000)
		{
			sum=(3*i)+2;
			if(sum%4!=0)
			{
				System.out.print(sum+" ");
				x=x+1;
			}
			i=i+1;
		}

	}
}
