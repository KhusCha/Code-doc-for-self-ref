package uni.codingNinja;

import java.util.Scanner;

public class CodingNinja {
	
	public static void main(String[] args) {

        
        Scanner s = new Scanner(System.in);
        
            int  i = 1 , result =1 ;

		

		int  n = s.nextInt();

		

		

		

		while(result<=n) {

			i++ ;

			result = i*i ;

			

		}

		 System.out.println( i- 1 );

	}
}
